        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Vnsb9aAGEe2yhtolEJTpxQ" name="updateCard" specification="_VlfchqAGEe2yhtolEJTpxQ">
          <ownedParameter xmi:id="_Vnsb9qAGEe2yhtolEJTpxQ" name="pos">
